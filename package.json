{
  "name": "greenfield",
  "version": "2.5.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "now-start": "serve -s ./build",
    "build": "rm -rf ./build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "lint": "standard --fix",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@curi/react": "^1.0.0-beta.29",
    "@curi/router": "^1.0.0-beta.37",
    "@curi/side-effect-scroll": "^1.0.0-beta.9",
    "@hickory/browser": "^1.0.0-beta.7",
    "axios": "^0.18.0",
    "fixed-data-table-2": "^0.8.14",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-data-structures": "^0.1.6",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.4.2",
    "styled-icons": "^2.3.2"
  },
  "devDependencies": {
    "react-scripts": "^1.1.4",
    "redux-devtools-extension": "^2.13.5",
    "serve": "^9.4.2",
    "standard": "^11.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/",
      "!src/state/store.js",
      "!src/state/sagas/*",
      "!src/index.js"
    ]
  }
}
